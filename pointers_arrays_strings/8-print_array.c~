#include <stdio.h>
#include "main.h"
/**
*print_array - Write a function that prints n elements of an array of integers,
*followed by a new line.
*@a: pointer
*@n: is the number of elements of the array to be printed
*/
void print_array(int *a, int n)
{
int i;
for (i = 0; i < n; i++)
{
printf("%d", a[i]);
if (i != n - 1)
{
_putchar(',');
_putchar(' ');
}
}
_putchar('\n');
}
