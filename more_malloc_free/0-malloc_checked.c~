#include "main.h"
#include <stddef.h>
#include <stdlib.h>
/**
**_strdup - returns a pointer to a newly allocated space in memory,
*which contains a copy of the string given as a parameter.
*@b:  
*Return: pointer
*/
void *malloc_checked(unsigned int b)
{
void *i;
i  = malloc(b);
if (i == NULL)
{
exit(98);
}
return (i);
}
